{
    "name": "codemath",
    "displayName": "CodeMath",
    "description": "Save time and effort calculating math problems in your development environment.",
    "version": "0.0.1",
    "icon": "./assets/logo.png",
    "galleryBanner": {
        "color": "#0A3649",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.79.0"
    },
    "categories": [
        "Other"
    ],
    "publisher": "otnansirk",
    "author": {
        "name": "otnansirk"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "codemath.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@types/vscode": "^1.79.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.2",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "advanced-calculator": "^1.1.0"
    }
}